# MySQL Database (RDS) - Required if not using SQLite
# If these are not set, the application will use SQLite as fallback
DB_HOST=your-rds-endpoint.rds.amazonaws.com
DB_PORT=3306
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_NAME=marketplace

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=False
BASE_URL=http://your-server-url:8000

# Processing Configuration
MAX_CONCURRENT_JOBS=1
MIN_IMAGES=3
MAX_IMAGES=50
MAX_IMAGE_SIZE=1600
TRAINING_ITERATIONS=10000
COLMAP_MAX_FEATURES=8192
COLMAP_NUM_THREADS=8
